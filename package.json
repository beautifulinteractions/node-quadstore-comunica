{
  "name": "quadstore-comunica",
  "version": "0.0.5",
  "description": "This library lets you use the Comunica query engine with quadstore. It is not meant to be used on its own but rather as a dependency of quadstore itself.",
  "main": "index.bundle.js",
  "typings": "index.d.ts",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "build-ts": "tsc --project tsconfig.json",
    "build-engine": "comunica-compile-config config/config-default.json  > engine.js",
    "build": "npm run build-ts && npm run build-engine && npm run bundle-js",
    "bundle-js": "webpack --config webpack.config.js",
    "analyze-bundle": "webpack --config webpack.config.js --profile --json > webpack.stats.json && webpack-bundle-analyzer webpack.stats.json",
    "publish:alpha": "npm publish --tag alpha",
    "publish:beta": "npm publish --tag beta"
  },
  "homepage": "https://github.com/beautifulinteractions/node-quadstore-comunica",
  "bugs": "https://github.com/beautifulinteractions/node-quadstore/issues",
  "repository": {
    "type": "git",
    "url": "https://github.com/beautifulinteractions/node-quadstore-comunica.git"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {},
  "devDependencies": {
    "@comunica/actor-init-sparql": "^1.18.0",
    "json-loader": "^0.5.7",
    "typescript": "^4.0.3",
    "webpack": "^5.0.0",
    "webpack-bundle-analyzer": "^3.9.0",
    "webpack-cli": "^4.0.0"
  },
  "peerDependencies": {
    "asynciterator": "^3.0.3",
    "sparqljs": "^3.1.1",
    "sparqlalgebrajs": "^2.4.0"
  },
  "files": [
    "config",
    "components",
    "index.js",
    "index.d.ts",
    "index.bundle.js",
    "engine.js"
  ],
  "lsd:module": "https://linkedsoftwaredependencies.org/bundles/npm/quadstore-comunica",
  "lsd:contexts": {
    "https://linkedsoftwaredependencies.org/bundles/npm/quadstore-comunica/^1.0.0/components/context.jsonld": "components/context.jsonld"
  },
  "lsd:importPaths": {
    "https://linkedsoftwaredependencies.org/bundles/npm/quadstore-comunica/^1.0.0/components/": "components/",
    "https://linkedsoftwaredependencies.org/bundles/npm/quadstore-comunica/^1.0.0/config/": "config/"
  }
}
